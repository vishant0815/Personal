<odoo>
    <data>
        <record id="view_library_book_tags_list" model="ir.ui.view">
            <field name="name">library.book.tags.list</field>
            <field name="model">library.book.tags</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_library_book_tags_form" model="ir.ui.view">
            <field name="name">library.book.tags.form</field>
            <field name="model">library.book.tags</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_library_book_tags" model="ir.actions.act_window">
            <field name="name">Book Tags</field>
            <field name="res_model">library.book.tags</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>